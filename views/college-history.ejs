<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>College Event History</title>
  <link rel="stylesheet" href="/css/college-history.css">
</head>
<body>
  <header>
    <h1>Event History</h1>
  </header>
  <main>
    <section id="event-history">
      <h2>Past Events</h2>
      <ul>
        <% historyEvents.forEach(event => { %>
          <li>
            <h2><%= event.name %></h2>
            <p><%= event.type %></p>
            <p><%= event.subject %></p>
            <p><%= event.location %></p>
            <p><%= event.eligibility%></p>
            <p><%= event.registrationfee %></p>
            <p><%= event.prizes %></p>
            <p>Date: <%= new Date(event.date).toLocaleDateString() %></p>
            <p><%= event.description %></p>
          </li>
        <% }) %>
      </ul>
    </section>
  </main>
</body>
</html>
